{
  "name": "@solana/account-compression",
  "version": "0.1.0",
  "description": "SPL Account Compression Program JS API",
  "author": "Solana Maintainers <maintainers@solana.foundation>",
  "repository": {
    "url": "https://github.com/solana-labs/solana-program-library",
    "type": "git"
  },
  "sideEffects": false,
  "engines": {
    "node": ">=16"
  },
  "license": "Apache-2.0",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/cjs/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/cjs/index.js",
      "import": "./dist/esm/index.js",
      "types": "./dist/cjs/index.d.ts"
    }
  },
  "scripts": {
    "build": "rm -rf dist/ && tsc && tsc -p tsconfig.cjs.json",
    "start-validator": "solana-test-validator --reset --quiet --bpf-program GRoLLzvxpxxu2PGNJMMeZPyMxjAUH9pKqxGXV9DGiceU ../target/deploy/spl_compression.so --bpf-program WRAPYChf58WFCnyjXKJHtrPgzKXgHp6MD9aVDqJBbGh ../target/deploy/wrapper.so",
    "run-tests": "jest tests --detectOpenHandles",
    "test": "start-server-and-test start-validator http://localhost:8899/health run-tests"
  },
  "dependencies": {
    "@metaplex-foundation/beet": "^0.6.1",
    "@types/bn.js": "^5.1.1",
    "bn.js": "^5.2.1",
    "borsh": "^0.7.0"
  },
  "peerDependencies": {
    "@solana/web3.js": "^1.50.1"
  },
  "devDependencies": {
    "@metaplex-foundation/rustbin": "^0.3.1",
    "@metaplex-foundation/solita": "^0.12.2",
    "@project-serum/anchor": "^0.25.0",
    "@types/chai": "^4.3.0",
    "@types/jest": "^29.0.0",
    "chai": "^4.3.4",
    "jest": "^29.0.1",
    "jest-config": "^29.0.1",
    "js-sha3": "^0.8.0",
    "start-server-and-test": "^1.14.0",
    "ts-jest": "^28.0.8",
    "ts-jest-resolver": "^2.0.0",
    "ts-node": "^10.9.1",
    "typescript": "=4.7.4",
    "typescript-collections": "^1.3.3"
  },
  "files": [
    "dist"
  ]
}